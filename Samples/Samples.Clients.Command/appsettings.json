{
  "AllowedHosts": "*",
  "GS": {
    "DecoupleIt": {
      "Scheduling": {
        "Logging": {
          "EnableNonErrorLogging": true
        }
      },
      "Operations": {
        "CommandDoNotCreateOwnScope": true,
        "Logging": {
          "EnableNonErrorLogging": true,
          "DebugRemap": {
            "GS.DecoupleIt.Operations.Internal.CommandDispatcher": "Information"
          },
          "ExceptionCategoryRemap": {
            "Business": "Information"
          }
        }
      },
      "Migrations": {
        "Enabled": true
      },
      "HttpAbstraction": {
        "SkipSSLCertificateValidation": true,
        "ServicesUris": {
          "Samples.Clients.Command": "http://localhost:5000"
        }
      },
      "AspNetCore": {
        "Service": {
          "Logging": {
            "Console": {
              "Enabled": true
            },
            "LogRequests": true,
            "LogResponses": true,
            "LogRequestsBodies": true,
            "LogResponsesBodies": true,
            "LogRequestsHeaders": false,
            "LogResponsesHeaders": false
          }
        }
      },
      "Contextual": {
        "UnitOfWork": {
          "LogStackTrace": false,
          "Pooling": {
            "Enabled": true,
            "Default": {
              "MaxPoolSize": 50
            }
          }
        }
      }
    }
  }
}
