{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "GS": {
    "DecoupleIt": {
      "Tracing": {
        "LoggerProperties": {
          "TraceId": [
            "TraceId",
            "X-B3-TraceId"
          ],
          "SpanId": [
            "SpanId",
            "X-B3-SpanId"
          ],
          "SpanName": [
            "SpanName",
            "X-B3-SpanName"
          ],
          "ParentSpanId": [
            "ParentSpanId",
            "X-B3-ParentSpanId"
          ]
        },
        "AspNetCore": {
          "Headers": {
            "TraceIdHeaderName": "traceId",
            "SpanIdHeaderName": "spanId",
            "SpanNameHeaderName": "spanName",
            "ParentSpanIdHeaderName": "parentSpanId"
          }
        }
      }
    }
  }
}
