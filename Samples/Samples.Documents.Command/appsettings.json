{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "GS": {
    "DecoupleIt": {
      "Tracing": {
        "AspNetCore": {
          "Headers": {
            "TraceIdHeaderName": "traceId",
            "SpanIdHeaderName": "spanId",
            "SpanNameHeaderName": "spanName",
            "ParentSpanIdHeaderName": "parentSpanId"
          }
        }
      },
      "HttpAbstraction": {
        "SkipSSLCertificateValidation": "true",
        "TraceIdHeaderName": "traceId",
        "SpanIdHeaderName": "spanId",
        "SpanNameHeaderName": "spanName",
        "ParentSpanIdHeaderName": "parentSpanId",
        "ServicesUris": {
          "Samples.Clients.Command": "https://localhost:5001"
        }
      }
    }
  }
}
