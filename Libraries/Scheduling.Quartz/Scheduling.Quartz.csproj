<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <VersionPrefix>3.0.1</VersionPrefix>
        <Title>DecoupleIt - Scheduling using Quartz</Title>
        <Description>Library uses Quartz as a scheduler for registered jobs using attributes from GS.DecoupleIt.Scheduling.</Description>
        <PackageReleaseNotes>
            - logging exception on error level for job execution
        </PackageReleaseNotes>
    </PropertyGroup>

    <ItemGroup>
        <ProjectReference Include="..\InternalEvents\InternalEvents.csproj" />
        <ProjectReference Include="..\Scheduling\Scheduling.csproj" />
        <ProjectReference Include="..\Tracing\Tracing.csproj" />
    </ItemGroup>

    <ItemGroup>
        <PackageReference Include="Quartz" Version="3.2.3" />
    </ItemGroup>

    <Import Project="../libraries.core.props" />

</Project>
